<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gabriellispa.appv1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://gabriellispa.it/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Gabrielli v1</name>
    <description>
        Gabrielli App v.1
    </description>
    <author email="l.lombardi@afbnet.it" href="http://www.afbnet.it">
        AfbNET dev team
    </author>
    <icon src="img/logo-gabrielli-gruppo.jpg" />
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
